{
  "extends": "../tsconfig",
  "include": [
    "src"
  ],
  "compilerOptions": {
    "lib": [
      "es2020",
    ],
    "rootDir": ".",
    "baseUrl": ".",
    "moduleResolution": "node16",
    "module": "es2020",
    "target": "es2020",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "paths": {
      "#auth/*": [
        "./src/auth/*"
      ],
    }
  }
}
